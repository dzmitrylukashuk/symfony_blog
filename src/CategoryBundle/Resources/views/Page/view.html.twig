{% extends '@App/Layout/layout-two-col-4-8.html.twig' %}

{% block regionRight %}
    <div class="block block--category-view">
        <div class="block-content">
            <h2 class="field field-title">{{ category.name }}</h2>
            <div class="field field-description">{{ category.description }}</div>
            <div class="field field-date_add">{{ category.dateAdd|date('d-m-Y') }}</div>
        </div>
    </div>
{% endblock %}